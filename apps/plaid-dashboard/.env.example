# Plaid API Credentials
# Get these from https://dashboard.plaid.com/developers/keys
PLAID_CLIENT_ID=your_client_id
PLAID_SECRET=your_secret
PLAID_ENVIRONMENT=sandbox  # sandbox | production

# Database (local Postgres via Docker)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/plaid_dashboard

# Engine API
ENGINE_URL=http://localhost:3001

# Next.js public env vars
NEXT_PUBLIC_PLAID_ENVIRONMENT=sandbox
NEXT_PUBLIC_ENGINE_URL=http://localhost:3001
